<Application x:Class="CefSharp.OutOfProcess.Wpf.OffscreenHost.Example.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converter="clr-namespace:CefSharp.OutOfProcess.Wpf.OffscreenHost.Example.Converter"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converter:TitleConverter x:Key="TitleConverter"/>
        <converter:EnvironmentConverter x:Key="EnvironmentConverter" />
        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    </Application.Resources>
</Application>
